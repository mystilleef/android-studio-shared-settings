<templateSet group="AndroidComments">
  <template name="ccode" value="&lt;code&gt;$code$&lt;/code&gt;" description="code tag for javadoc" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="code" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="cfalse" value="&lt;code&gt;false&lt;/code&gt;" description="puts false in a comment section" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <context />
  </template>
  <template name="clink" value="{@link $classToLink$}" description="link from within javadoc" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="classToLink" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
  <template name="ctrue" value="&lt;code&gt;true&lt;/code&gt;" description="puts true in a comment section" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <context />
  </template>
</templateSet>